<table>
<tbody>
{{#each session in controller}}
<tr>
  <td class="name">{{session.name}}</td>
</tr>
<tr>
  <td class="room">{{session.room}}</td>
</tr>
<tr>
  {{#each speaker in session.speakers}}
  <td class="speaker">{{speaker.name}}</td>
  {{/each}}
</tr>
<tr>
  {{#each rating in session.ratings}}
  <td class="rating">{{rating.score}}</td>
  {{/each}}
</tr>
<tr>
  {{#each tag in session.tags}}
  <td class="tag">{{tag.description}}</td>
  {{/each}}
</tr>
<tr>
  <td>{{#link-to 'session' session}}View Session Details{{/link-to}}</td>
</tr>
{{/each}}
</tbody>
</table>

<div class="associations">
{{#link-to 'associations'}}View All Associations{{/link-to}}
</div>

<div class="joel">
{{#link-to 'speakers'}}View Details About Joel{{/link-to}}
</div>
